		double a, b, c;
		
		System.out.println("Informe os lados do Triangulo X: ");
		a = sc.nextDouble();
		b = sc.nextDouble();
		c = sc.nextDouble();
		
		triangleX = new Triangle(a,b,c);
		
		System.out.println("Informe os lados do Triangulo X: ");
		a = sc.nextDouble();
		b = sc.nextDouble();
		c = sc.nextDouble();
		
		triangleY = new Triangle(a,b,c);
		
		
		
		

	}

